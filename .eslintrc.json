{
  "extends": "airbnb",
  "env": {
    "es6": true,
    "browser": true,
    "mocha": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "rules": {
    "indent": [
      2,
      4,
      {
        "SwitchCase": 1
      }
    ],
    "react/jsx-indent-props": [
      2,
      4
    ],
    "react/jsx-filename-extension": [
      0
    ],
    "react/jsx-indent": [
      2,
      4
    ],
    "jsx-a11y/no-static-element-interactions": [
      1
    ],
    "no-use-before-define": [
      0
    ],
    "max-len": [
      2,
      120
    ],
    "import/extensions": [
      2
    ],
    "no-param-reassign": [
      2,
      {
        "props": false
      }
    ]
  }
}